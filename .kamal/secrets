SECRETS=$(kamal secrets fetch --adapter bitwarden-sm all)

KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD $SECRETS)

DATABASE_URL=$(kamal secrets extract DATABASE_URL $SECRETS)

CACHE_URL=$(kamal secrets extract CACHE_URL $SECRETS)
QUEUE_URL=$(kamal secrets extract QUEUE_URL $SECRETS)
CABLE_URL=$(kamal secrets extract CABLE_URL $SECRETS)

POSTGRES_PASSWORD=$(kamal secrets extract POSTGRES_PASSWORD $SECRETS)

RAILS_MASTER_KEY=$(cat config/master.key)
